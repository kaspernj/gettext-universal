{
  "bin": {
    "gettext-universal": "bin/gettext-universal.js"
  },
  "name": "gettext-universal",
  "type": "module",
  "version": "1.0.20",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/**"
  ],
  "scripts": {
    "build": "rm -rf build && tsc && npm run chmod-executables",
    "chmod-executables": "chmod +x build/bin/gettext-universal.js && chmod +x build/bin/po2js.js",
    "gettext-universal": "node bin/gettext-universal.js",
    "po2js": "node bin/po2js.js",
    "prepublishOnly": "npm run build",
    "test": "jasmine",
    "typecheck": "tsc --noEmit",
    "watch": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaspernj/gettext-universal.git"
  },
  "keywords": [
    "gettext",
    "poedit",
    "scan"
  ],
  "author": "kasper@diestoeckels.de",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kaspernj/gettext-universal/issues"
  },
  "homepage": "https://github.com/kaspernj/gettext-universal#readme",
  "description": "",
  "dependencies": {
    "diggerize": "^1.0.5",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "jasmine": "^5.7.1",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@kaspernj/api-maker": "*",
    "expo-localization": "*",
    "react": "*"
  }
}
